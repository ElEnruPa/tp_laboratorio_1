/*3-2
int validarParidad(int num);

int main(void)
{
   setbuf(stdout,NULL);

   int num;
   int respuesta;

   printf("Ingrese un numero: ");
   fflush(stdin);
   scanf("%d",&num);

   respuesta=validarParidad(num);
   printf("%d",respuesta);

   return EXIT_SUCCESS;
}

int validarParidad(int num)
{
   int paridad;
   paridad=0;

   if(num%2==0)
   {
      paridad=1;
   }

   return paridad;
}
*/


   printf("Ingrese 3 numeros: ");
   scanf("%d",&a);
   scanf("%d",&b);
   scanf("%d",&c);

   if(a>b && a>c)
   {
      mayor=a;
   }
   else
   {
      if(b>c)
      {
         mayor=b;
      }
      else
      {
         mayor=c;
      }
   }

   printf("El numero mayor es: %d", mayor);

   return EXIT_SUCCESS;
}

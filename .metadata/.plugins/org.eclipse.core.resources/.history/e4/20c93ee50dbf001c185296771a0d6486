/*3-4
int ingresoEntero();
int validarNumero(int);

int main(void)
{
   setbuf(stdout,NULL);
   int respuesta;

   respuesta=ingresoEntero();

   printf("%d", respuesta);

   return EXIT_SUCCESS;
}

int ingresoEntero()
{
   int num;

   printf("Ingrese un numero de 0 a 10: ");
   fflush(stdin);
   scanf("%d", &num);

   num=validarNumero(num);

   return num;
}

int validarNumero(int num)
{
   while(num<0 || num>10)
   {
      printf("Ingrese BIEN un numero de 0 a 10: ");
      fflush(stdin);
      scanf("%d", &num);
   }
   return num;
}
*/

/*4-1
int ingresoEntero();
int validarNumero(int);

int main(void)
{
   setbuf(stdout,NULL);

   int numero;
   int factorial;
   int i;
   factorial=1;

   numero=ingresoEntero();

   for(i=1;i<=numero;i++)
   {
      factorial*=i;
      printf("%d \n", factorial);
   }

   return EXIT_SUCCESS;
}


int ingresoEntero()
{
   int num;

   printf("Ingrese un numero de 0 a 10: ");
   fflush(stdin);
   scanf("%d", &num);

   num=validarNumero(num);

   return num;
}

int validarNumero(int num)
{
   while(num<0 || num>10)
   {
      printf("Ingrese BIEN un numero de 0 a 10: ");
      fflush(stdin);
      scanf("%d", &num);
   }
   return num;
}
*/


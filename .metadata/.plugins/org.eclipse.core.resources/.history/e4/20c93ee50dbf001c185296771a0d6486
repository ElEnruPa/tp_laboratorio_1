/*1-3
int main(void)
{
   setbuf(stdout,NULL);

   int a;
   int b;
   int c;
   int mayor;
   int medio;
   int menor;

   printf("Ingrese 3 numeros: ");
   scanf("%d",&a);
   scanf("%d",&b);
   scanf("%d",&c);

   if(a>b && a>c)
   {
      mayor=a;
   }
   else
   {
      if(b>c && b>a)
      {
         mayor=b;
      }
      else
      {
         if(c>b && c>a)
         {
            mayor=c;
         }
      }
   }

   if(a<b && a<c)
   {
      menor=a;
   }
   else
   {
      if(b<c && b<a)
      {
         menor=b;
      }
      else
      {
         if(c<b && c<a)
         {
            menor=c;
         }
      }
   }

   if(a==b || a==c || b==c)
   {
      printf("No existe numero medio");
   }
   else
   {
      medio=(a+b+c-mayor-menor);
      printf("El numero del medio es: %d", medio);
   }


   return EXIT_SUCCESS;
}
*/

/*2-1
int main(void)
{
   setbuf(stdout,NULL);

   int a;
   int b;
   int c;
   int d;
   int e;
   int suma;
   float promedio;

   printf("Ingrese 5 numeros: ");
   scanf("%d",&a);
   scanf("%d",&b);
   scanf("%d",&c);
   scanf("%d",&d);
   scanf("%d",&e);

   suma=a+b+c+d+e;
   promedio=suma/(float)5;

   printf("La suma de esos numeros es: %d \n", suma);
   printf("El promedio de esos numeros es: %f", promedio);

   return EXIT_SUCCESS;
}
*/

/*2-2
int main(void)
{
   setbuf(stdout,NULL);

   int numero;
   int cantidadPares;
   int cantidadImpares;
   int menorNumero;
   int flagMenorNum;
   int mayorPar;
   int flagMayorPar;
   int sumaPositivos;
   int productoNegativos;
   int i;
   cantidadPares=0;
   flagMenorNum=0;
   flagMayorPar=0;
   sumaPositivos=0;
   productoNegativos=1;

   for(i=0;i<10;i++)
   {
      printf("Escribe un numero distinto de 0: ");
      scanf("%d",&numero);

      while(numero==0)
      {
         printf("Escriba bien el numero: ");
         scanf("%d",&numero);
      }

      if(numero%2==0)
      {
         cantidadPares++;
      }

      if(flagMenorNum==0)
      {
         menorNumero=numero;
         flagMenorNum++;
      }
      else
      {
         if(numero<menorNumero)
         {
            menorNumero=numero;
         }
      }

      if(numero%2==0 && flagMayorPar==0)
      {
         mayorPar=numero;
         flagMayorPar++;
      }
      else
      {
         if(numero%2==0 && numero>mayorPar)
         {
            mayorPar=numero;
         }
      }

      if(numero>0)
      {
         sumaPositivos+=numero;
      }
      else
      {
         productoNegativos*=numero;
      }
   }

   cantidadImpares=10-cantidadPares;

   printf("La cantidad de numeros pares son: %d \n",cantidadPares);
   printf("La cantidad de numeros impares son: %d \n",cantidadImpares);
   printf("El menor numero ingresado es: %d \n", menorNumero);
   printf("El mayor numero par ingresado es: %d \n", mayorPar);
   printf("La suma de los numeros positivos es: %d \n", sumaPositivos);
   printf("El producto de los numeros negativos son: %d \n", productoNegativos);


   return EXIT_SUCCESS;
}
*/

/*2-3
int main(void)
{
   setbuf(stdout,NULL);

   int numCliente;
   char estadoCivil;
   int edad;
   int temperatura;
   char genero;
   int precio;
   char seguir;
   int cantidadA;
   int numClientJoven;
   int edadMujerJoven;
   int flagB;
   int totalPrecio;
   int pasajero60;
   int numPasajeros;
   int promedio;
   int precioDescuento;
   precio=600;
   seguir='s';
   cantidadA=0;
   flagB=0;
   totalPrecio=0;
   pasajero60=0;
   numPasajeros=0;



   while(seguir=='s')
   {
      printf("Ingrese el numero de cliente: ");
      scanf("%d",&numCliente);

      printf("Ingrese el estado civil('s' para soltero, 'c' para casado o 'v' viudo): \n");
      fflush(stdin);//para q funcione bien(primero hace el scanf y dsp entra al while)(creo q solo para char)
      scanf("%c",&estadoCivil);

      while(estadoCivil!='s' && estadoCivil!='c' && estadoCivil!='v')
      {
         printf("Ingrese BIEN el estado civil('s' para soltero, 'c' para casado o 'v' viudo): \n");
         fflush(stdin);
         scanf("%c",&estadoCivil);
      }

      printf("Ingrese la edad ( solo mayores de edad, más de 17): \n");
      scanf("%d",&edad);

      while(edad<18)
      {
         printf("Ingrese BIEN la edad ( solo mayores de edad, más de 17): \n");
         scanf("%d",&edad);
      }

      printf("Ingrese la temparatura corporal: \n");
      scanf("%d",&temperatura);

      printf("Ingrese el genero ('f' para femenino, 'm' para masculino, 'o' para no binario): \n");
      fflush(stdin);
      scanf("%c",&genero);

      while(genero!='f' && genero!='m' && genero!='o')
      {
         printf("Ingrese BIEN el genero ('f' para femenino, 'm' para masculino, 'o' para no binario): \n");
         fflush(stdin);
         scanf("%c",&genero);
      }

      printf("Desea seguir ingresando clientes?? ('s' para si, 'n' para no): \n");
      fflush(stdin);
      scanf("%c",&seguir);

      if(estadoCivil=='v' && edad>60)
      {
         cantidadA++;
      }

      if(genero=='f' && estadoCivil=='s' && flagB==0)
      {
         numClientJoven=numCliente;
         edadMujerJoven=edad;
         flagB++;
      }
      else
      {
         if(genero=='f' && estadoCivil=='s' && edad<edadMujerJoven)
         {
            edadMujerJoven=edad;
            numClientJoven=numCliente;
         }
      }

      totalPrecio+=precio;

      numPasajeros++;

      if(edad>60)
      {
         pasajero60++;
      }

      while(seguir!='s' && seguir!='n')
      {
         printf("Desea seguir ingresando clientes?? ESCRIBI BIEN ('s' para si, 'n' para no): \n");
         fflush(stdin);
         scanf("%c",&seguir);
      }

   }

   promedio=(pasajero60*100)/numPasajeros;

   printf("La cantidad de personas viudas con mas de 60 años son: %d \n", cantidadA);
   printf("El numero de cliente de la mujer mujer soltera más joven es: %d \n", numClientJoven);
   printf("La edad de la mujer mujer soltera más joven es: %d \n", edadMujerJoven);
   printf("El precio total sin descuento es: %d \n", totalPrecio);

   if(promedio>50)
   {
      precioDescuento=totalPrecio*0.75;
      printf("El precio fnal con descuento es de: %d \n",precioDescuento);
   }

   return EXIT_SUCCESS;
}
*/

/*2-4
int main(void)
{
   setbuf(stdout,NULL);

   int i;
   int contador;
   char caracter;
   contador=0;

   for(i=0;i<5;i++)
   {
      printf("Ingrese una letra: ");
      fflush(stdin);
      scanf("%c",&caracter);

      if(caracter=='p')
      {
         contador++;
      }
   }

   printf("Se ingresaron %d letras 'p' ", contador);

   return EXIT_SUCCESS;
}
*/

/*3-1
void mostrarNumFlotante(float numero);

int main(void)
{
   setbuf(stdout,NULL);

   float numero;

   printf("Ingrese un numero flotante: ");
   fflush(stdin);
   scanf("%f",&numero);

   mostrarNumFlotante(numero);

   return EXIT_SUCCESS;
}

mostrarNumFlotante(float numero)
{
   printf("El numero ingresado es: %f", numero);
}
